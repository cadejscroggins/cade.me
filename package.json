    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "a-cdk": "^0.1.53",
    "a-conf": "^0.1.14",
    "a-uik": "^0.1.23",
    "aws-amplify": "^3.3.17",
    "aws-cdk": "^1.88.0",
    "aws-sdk": "^2.838.0",
    "framer-motion": "^3.3.0",
    "graphql": "^15.5.0",
    "next": "^10.0.6",
    "next-seo": "^4.19.0",
